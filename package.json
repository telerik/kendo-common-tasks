{
  "name": "@telerik/kendo-common-tasks",
  "description": "Build infrastructure helpers and gulp tasks for Kendo UI Angular 2 / React components",
  "version": "0.0.0-semantically-released",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-common-tasks.git"
  },
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./*.js ./docs-public/*[!min].js",
    "semantic-release": "semantic-release pre && semantic-prerelease publish && semantic-release post",
    "test": "npm run lint"
  },
  "keywords": [
    "Kendo UI"
  ],
  "dependencies": {
    "@telerik/eslint-config": "1.1.0",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.0.0",
    "babel-eslint": "^4.1.6",
    "browser-sync": "^2.10.0",
    "browser-sync-webpack-plugin": "^1.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.1",
    "eslint-plugin-react": "^3.10.0",
    "express": "^4.13.4",
    "express-urlrewrite": "^1.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "glob": "^6.0.4",
    "graphlib": "2.1.1",
    "gulp": "^3.9.0",
    "gulp-add-src": "^0.2.0",
    "gulp-contains": "^1.1.0",
    "gulp-eslint": "^1.1.0",
    "gulp-if": "^2.0.0",
    "gulp-load-plugins": "1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
